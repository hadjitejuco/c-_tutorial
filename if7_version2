/*
Display the Grade value base on grade range
Range           grade
90 -100         A
80 – 89         B
70 – 79         C
60 – 69         D
50 - 59         F
below 50        invalid range
*/
//logical operator (&&-AND)
#include<iostream>
using namespace std;
int main(){
    int grade;
    cout << " Enter grade: ";
    cin >> grade;
    if (grade>=90 && grade<=100)
        cout << "\n A";
    else if ( grade>=80 && grade<=89)
        cout << "\n B";
    else if (grade>=70 && grade<=79)
        cout << " C";
    else if (grade>=60 && grade<=69)
        cout << "\n D";
    else if (grade>=50 && grade<=59)
        cout << "F";
    else
        cout << "Invalid Range";
    return 0;

}
