// Program to use Comma ( , ) operator

#include <iostream>
using namespace std;

int main()
{
	int num, sq, cube; 
	num = 10;

	sq = (num * num), cube = (num * num * num);
	
	cout << "The square of " << num << " is ::: " << sq;
	cout << "\nThe cube of " << num << " is ::: " << cube << "\n";
	
	return 0;
}
