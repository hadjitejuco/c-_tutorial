// Program to demonstrate the use of Assignment Operator ( = )

#include <iostream>
using namespace std;

int main()
{
	int a, b, c, d, e, f;

	a = b = c = d = e = f = 10;
	
	cout << "\n a = " << a << ", b = " << b;
	cout << "\n c = " << c << ", d = " << d;
	cout << "\n e = " << e << ", f = " << f;
	
	return 0;
}

