// This program demostrate how to use toCascading of Insertion ( << )
// operator and Cascading of Extraction ( >> ) operators

#include <iostream>
using namespace std;
int main()
{
	int num1, num2, sum;

	cout << "\n Enter Two Numbers --> ";
	cin >> num1 >> num2;

	sum = num1 + num2;

	cout << "\n " << num1 << " + " << num2 << " = " << sum;

	return 0;
}
